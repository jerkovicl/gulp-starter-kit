{
  "name": "Starter kit - bower.json",
  "private": true,
  "version": "1.0.0",
  "description": "bower.json example file for Starter kit",
  "authors": [
    "Luka Jerkovic"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jerkovicl/gulp-starter-kit",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    "package.json",
    "src"
  ],
  "devDependencies": {
    "bootstrap": "~3.3.1",
    "owl-carousel2": "latest",
    "moment": "~2.8.4",
    "matchHeight": "latest",
    "magnific-popup": "latest",
    "modernizr": "latest",
    "select2": "latest"
  },
  "dependencies": {
    "jquery": "~2.1.1",
    "bootstrap": "~3.3.1",
    "owl-carousel2": "latest",
    "moment": "~2.8.4",
    "matchHeight": "latest",
    "magnific-popup": "latest",
    "modernizr": "latest",
    "kendo-ui-core": "latest"
  },
  "exportsOverride": {

        "bootstrap": {
          "main": [
            "./dist/js/*.*",
            "./dist/css/*.*",
            "./dist/fonts/*.*"
          ]
        },
        "jquery": {
          "main": [
            "./dist/jquery.{js,min.js,min.map}"
          ]
        },
        "select2": {
          "main": [
          "./dist/js/*.*"
          ]
        }
  }
}
